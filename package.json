{
  "name": "Selenidejs",
  "description": "Decorator for Selenium Webdriver",
  "homepage": "https://github.com/KnowledgeExpert/selenidejs",
  "version": "0.2.0",
  "main": "built/index.js",
  "typings": "built/index.d.ts",
  "dependencies": {
    "@types/fs-extra": "5.0.3",
    "@types/selenium-webdriver": "3.0.10",
    "fs-extra": "6.0.1",
    "jimp": "0.2.28",
    "merge-img": "2.1.2",
    "selenium-webdriver": "4.0.0-alpha.1",
    "typescript": "2.5.3",
    "xpather": "0.0.1"
  },
  "devDependencies": {
    "jasmine-cookies": "0.1.3",
    "http-server": "0.11.1",
    "coveralls": "3.0.1",
    "tslint": "5.11.0",
    "nyc": "12.0.2"
  },
  "scripts": {
    "clean": "rm -rf ./built && rm -f package-lock.json",
    "compile-test": "tsc --project test",
    "compile": "tsc --project lib",
    "startserver": "http-server ./resources -p 4445",
    "pretest": "npm run clean && npm run compile-test",
    "lint": "tslint --project lib",
    "test": "nyc --reporter=lcov --check-coverage --lines 50 --functions 25 --branches 30 jasmine ./built/test/**/*.test.js",
    "build": "npm run lint && npm run test && npm run clean && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/KnowledgeExpert/selenidejs.git"
  },
  "keywords": [
    "test",
    "testing",
    "webdriver",
    "webdriverjs",
    "selenium"
  ],
  "author": {
    "name": "Alexander Popov",
    "email": "aleksander.popov@k-expert.com"
  },
  "license": "Apache-2.0"
}
